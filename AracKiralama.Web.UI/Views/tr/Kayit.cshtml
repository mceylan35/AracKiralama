
@{
    ViewBag.Title = "Kayit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section id="page-title-area" class="section-padding overlay">
    <div class="container">
        <div class="row">
            <!-- Page Title Start -->
            <div class="col-lg-12">
                <div class="section-title  text-center">
                    <h2>Kayit ol!</h2>
                    <span class="title-line"><i class="fa fa-car"></i></span>
                    
                </div>
            </div>
            <!-- Page Title End -->
        </div>
    </div>
</section>
<!--== Login Page Content Start ==-->
<section id="lgoin-page-wrap" class="section-padding">
    <div class="container">
        <div class="row">
            <div class="col-lg-5 col-md-8 m-auto">
                <div class="login-page-content">
                    <div class="login-form">
                        <h3>Kayıt ol!</h3>
                        <form action="index.html">
                            <div class="name">
                                <div class="row">
                                    <div class="col-md-6">
                                        <input type="text" placeholder="Ad">
                                    </div>
                                    <div class="col-md-6">
                                        <input type="text" placeholder="Soyad">
                                    </div>
                                </div>
                            </div>
                            <div class="username">
                                <input type="email" placeholder="Email">
                            </div>
                            <div class="password">
                                <input type="password" placeholder="Sifre">
                            </div>
                            <div class="username">
                                <input type="text" placeholder="EhliyetAlimTarihi">
                            </div>
                            <div class="username">
                                <input type="text" placeholder="DogumTarihi">
                            </div>


                            <div class="log-btn">
                                <button type="submit"><i class="fa fa-check-square"></i> Kayıt ol!</button>
                            </div>
                        </form>
                    </div>

                    <div class="login-other">
                        <span class="or">or</span>
                        <a href="#" class="login-with-btn facebook"><i class="fa fa-facebook"></i> Facebook</a>
                        <a href="#" class="login-with-btn google"><i class="fa fa-google"></i> Google</a>
                    </div>
                    <div class="create-ac">
                        <p>Hesabın var mı?<a href="~/tr/giris">Giriş yap!</a></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--== Login Page Content End ==-->
